  other-extensions:    CPP
  build-depends:       base >=4.9 && <4.10, array >=0.5 && <0.6, containers >=0.5 && <0.6, unordered-containers >=0.2 && <0.3, linear < 2, lens
  hs-source-dirs:      src
  build-tools:         alex, happy
  other-modules:       Parser, Lexer
  default-language:    Haskell2010
